<div class="container vaccine-container">
  <div class="row">
    <div class="container my-3">
      <div class="row">
        <% @members.each do |member| %>
          <% vaccine_filter(member).each do |v| %>
            <div class="col-12 col-sm-6 text-center py-2 flash-pos">
              <div class="flash flash-danger alert-dismissible fade show animated slow     bounce delay-5s alert-x" role="alert">
                <span><strong><%= member.first_name %>:</strong> EstÃ¡ com a vacina <%= v.name%> atrasada! ğŸ˜± </span>
                <a data-dismiss="alert" aria-label="Close">
                  <i class="fas fa-times"></i>
                </a>
              </div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
      <div class="row">
        <% @members.each do |member| %>
          <%= link_to member_path(member) do %>
            <div class="col-12 col-sm-6 text-center justify-content-between ">
              <div class= 'btn d-flex flex-column justify-content-center' >
                <div class="avatar-card">
                  <% if member.photo.url %>
                    <%= cl_image_tag member.photo, class: "rounded-circle img-fluid" %>
                  <% else %>
                    <%= image_tag "default.jpg", class: "rounded-circle img-fluid"  %>
                  <% end %>
                </div>
                <div class="p-2">
                  <%= member.first_name %>
                </div>
              </div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
