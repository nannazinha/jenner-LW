<div class="container vaccine-container">
  <div class="row">
    <div class="container my-3">
      <div class="row">
        <% @members.each do |member| %>
          <% if vaccine_filter(member).size != 0 %>
            <div class="col-12 col-sm-6 text-center py-2 flash-pos">
              <div class="flash flash-danger alert-dismissible fade show animated slow     bounce delay-5s alert-x" role="alert">
                <span><strong><%= member.first_name %>:</strong> EstÃ¡ com alguma vacina atrasada! ğŸ˜± </span>
                <a data-dismiss="alert" aria-label="Close">
                </a>
              </div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="member-cont my-5">
    <% @members.each do |member| %>
      <%= link_to member_path(member) do %>

        <div class="card-member m-3">
          <div class= 'card-content' >

              <% if member.photo.url %>
                <%= cl_image_tag member.photo, class: "rounded-circle img-fluid" %>
              <% else %>
                <%= image_tag "default.jpg", class: "rounded-circle img-fluid"  %>
              <% end %>
              <h3 class="mt-3"> <b> <%= member.first_name %></b> </h3>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
